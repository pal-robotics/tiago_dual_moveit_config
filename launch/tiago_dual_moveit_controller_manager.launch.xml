<launch>
 <arg name="moveit_controller_manager" default="moveit_simple_controller_manager/MoveItSimpleControllerManager"/>
 <param name="moveit_controller_manager" value="$(arg moveit_controller_manager)"/>
 <arg name="robot_sufix"  default=""/>
 <rosparam unless="$(eval robot_sufix.startswith('custom'))" file="$(find tiago_dual_moveit_config)/config/controllers/controllers_$(arg robot_sufix).yaml"/>
 <rosparam if="$(eval robot_sufix.startswith('custom'))" file="$(find moveit_custom_dual_config)/config/controllers/controllers_custom.yaml"/>
</launch>
